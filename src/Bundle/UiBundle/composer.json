{
    "name": "php-lug/ui-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Lug UI bundle",
    "autoload": {
        "psr-4": { "Lug\\Bundle\\UiBundle\\": "" }
    },
    "require": {
        "php": "^5.6|^7.0",
        "symfony/config": "^3.0",
        "symfony/dependency-injection": "^3.0",
        "symfony/http-kernel": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "symfony/phpunit-bridge": "^3.0",
        "symfony/form": "^3.0",
        "knplabs/knp-menu-bundle": "^2.1"
    },
    "suggest": {
        "symfony/form": "Add icon support",
        "knplabs/knp-menu-bundle": "Add menu builder support"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
